#include<stdio.h>
int main()
{
int number;
int min, max;
printf("enter the maximum range:");
scanf("%d",&min);
printf("enter the maximum range:");
scanf("%d",&max);
printf("enter the odd number:");
for(number=min;number<max;number++)
if(number %2!=0)
printf("%d",number);
printf("enter the even number:");
for(number=min;number<max;number++)
if(number %2!=0);
printf("%d", number);
return 0;
}
